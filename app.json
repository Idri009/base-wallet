{
  "expo": {
    "name": "MoonPouch",
    "slug": "moon-pouch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "8695d127-8b77-4318-81c8-4025d9ed17ea"
      }
    },
    "android": {
      "package": "com.aniebietabasi.moonpouch"
    }
  }
}
